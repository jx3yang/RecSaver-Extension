{
  "name": "Recommendations Saver",
  "description": "Saves your previous Youtube Recommendations",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "Popup.html"
  },
  "options_page": "Options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["sendContents.js"]
    }
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ]
}
